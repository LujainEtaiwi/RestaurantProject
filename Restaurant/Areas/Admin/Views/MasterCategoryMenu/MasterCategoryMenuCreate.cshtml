@model Restaurant.Models.MasterCategoryMenu

@{
    ViewData["Title"] = "MasterCategoryMenuCreate";
}

<div class="pagetitle">
    <h1>category Menu Table</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <li class="breadcrumb-item active">category Menu</li>
        </ol>
    </nav>
</div>

<hr />

<section class="section">
    <div class="row">
        <div class="card">
            <div class="card-body">

                <form asp-action="MasterCategoryMenuCreate" class="needs-validation" novalidate>
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input asp-for="MasterCategoryMenuId" class="form-control" type="hidden" value="0" />
                    <div class="row mb-3">
                        <div class="col-6">
                            <div class="row mb-3">
                                <label asp-for="MasterCategoryMenuName" class="col-sm-4 col-form-label"></label>
                                <div class="col-sm-8">
                                    <input asp-for="MasterCategoryMenuName" class="form-control" required />
                                    <span asp-validation-for="MasterCategoryMenuName" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group text-end">
                        <input type="submit" value="Create" class="btn btn-primary" />
                        <a asp-action="MasterCategoryMenuIndex" class="btn btn-secondary">Back to List</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>


