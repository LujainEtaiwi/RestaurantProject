@model IEnumerable<Restaurant.Models.TransactionContactUs>

@{
    ViewData["Title"] = "TransactionContactUsIndex";
}

<div class="pagetitle">
    <h1>Transaction ContactUs Table</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <li class="breadcrumb-item active">Transaction ContactUs</li>
        </ol>
    </nav>
</div>


<hr />


<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <table class="table datatable index-table text-break">
                        <thead>
                            <tr >
                                <th>
                                    @Html.DisplayNameFor(model => model.TransactionContactUsFullName)
                                </th>
                                <th class="w-25">
                                    @Html.DisplayNameFor(model => model.TransactionContactUsEmail)
                                </th>
                                <th class="w-25">
                                    @Html.DisplayNameFor(model => model.TransactionContactUsSubject)
                                </th>
                                <th class="w-25">
                                    @Html.DisplayNameFor(model => model.TransactionContactUsMessage)
                                </th>
                            </tr>
                        </thead>
                        <tbody >
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td class="align-middle">
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsFullName)
                                    </td>
                                    <td class="align-middle">
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsEmail)
                                    </td>
                                    <td class="align-middle w-25">
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsSubject)
                                    </td>
                                    <td class="align-middle w-25">
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsMessage)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
